<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>References (NV)</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://www.commcarehq.org/a/corpora/tests/references" uiVersion="1" version="711" name="References (NV)">
					<a/>
					<b/>
					<c/>
					<group>
						<d/>
						<e/>
					</group>
					<f/>
				<orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/><cc:location/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/>
			<bind nodeset="/data/a"/>
			<bind nodeset="/data/b" type="xsd:string" relevant="/data/a = 'y'"/>
			<bind nodeset="/data/c" type="xsd:string" relevant="/data/a = 'y'"/>
			<bind nodeset="/data/group"/>
			<bind nodeset="/data/group/d"/>
			<bind nodeset="/data/group/e" type="xsd:string" relevant="/data/group/d = 'y'"/>
			<bind nodeset="/data/f" type="xsd:string" relevant="/data/group/d = 'y'"/>
			<itext>
				<translation lang="en" default="">
					<text id="a-label">
						<value>Show questions b and c?</value>
					</text>
					<text id="a-undefined-label">
						<value>Yes</value>
					</text>
					<text id="a-undefined-label2">
						<value>No</value>
					</text>
					<text id="b-label">
						<value>Question B</value>
					</text>
					<text id="c-label">
						<value>Question C</value>
					</text>
					<text id="group/d-label">
						<value>Show questions e and f?</value>
					</text>
					<text id="group/e-label">
						<value>Question E</value>
					</text>
					<text id="f-label">
						<value>Question F</value>
					</text>
				</translation>
				<translation lang="hin">
					<text id="a-label">
						<value>Show questions b and c?</value>
					</text>
					<text id="a-undefined-label">
						<value>Yes</value>
					</text>
					<text id="a-undefined-label2">
						<value>No</value>
					</text>
					<text id="b-label">
						<value>Question B</value>
					</text>
					<text id="c-label">
						<value>Question C</value>
					</text>
					<text id="group/d-label">
						<value>Show questions e and f?</value>
					</text>
					<text id="group/e-label">
						<value>Question E</value>
					</text>
					<text id="f-label">
						<value>Question F</value>
					</text>
				</translation>
			</itext>
		<setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/><orx:pollsensor event="xforms-ready" ref="/data/meta/location"/><bind nodeset="/data/meta/location" type="geopoint"/></model>
	</h:head>
	<h:body>
		<select1 ref="/data/a">
			<label ref="jr:itext('a-label')">Show questions b and c?</label>
			<item>
				<label ref="jr:itext('a-undefined-label')">Yes</label>
				<value>y</value>
			</item>
			<item>
				<label ref="jr:itext('a-undefined-label2')">No</label>
				<value>n</value>
			</item>
		</select1>
		<input ref="/data/b">
			<label ref="jr:itext('b-label')">Question B</label>
		</input>
		<input ref="/data/c">
			<label ref="jr:itext('c-label')">Question C</label>
		</input>
		<select1 ref="/data/group/d">
			<label ref="jr:itext('group/d-label')">Show questions e and f?</label>
			<item>
				<label ref="jr:itext('a-undefined-label')">Yes</label>
				<value>y</value>
			</item>
			<item>
				<label ref="jr:itext('a-undefined-label2')">No</label>
				<value>n</value>
			</item>
		</select1>
		<input ref="/data/group/e">
			<label ref="jr:itext('group/e-label')">Question E</label>
		</input>
		<input ref="/data/f">
			<label ref="jr:itext('f-label')">Question F</label>
		</input>
	</h:body>
</h:html>