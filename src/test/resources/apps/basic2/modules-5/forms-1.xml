<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Rate Case</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/C95D74C1-B0BE-4575-86E4-27A8BAC942BE" uiVersion="1" version="711" name="Rate Case">
					<basics>
						<rate_site/>
						<comment/>
					</basics>
					<last_inspection/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><comment/><last_inspection/><rate_site/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/><cc:location/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/><instance id="casedb" src="jr://instance/casedb"/>
			<bind nodeset="/data/basics"/>
			<bind nodeset="/data/basics/rate_site"/>
			<bind nodeset="/data/basics/comment" type="xsd:string"/>
			<bind nodeset="/data/last_inspection" calculate="today()"/>
			<itext>
				<translation lang="en" default="">
					<text id="basics-label">
						<value>Basic Review</value>
					</text>
					<text id="question2-label">
						<value>Choose a rating:</value>
					</text>
					<text id="question2-one-label">
						<value>1 - Poor</value>
					</text>
					<text id="question2-two-label">
						<value>2 - Needs Improvement</value>
					</text>
					<text id="question2-three-label">
						<value>3 - Average</value>
					</text>
					<text id="question2-four-label">
						<value>4 - Good</value>
					</text>
					<text id="question2-five-label">
						<value>5 - Exceptional</value>
					</text>
					<text id="comment-label">
						<value>Enter some text:</value>
					</text>
				</translation>
				<translation lang="hin">
					<text id="basics-label">
						<value>Basic Review</value>
					</text>
					<text id="question2-label">
						<value>Choose a rating:</value>
					</text>
					<text id="question2-one-label">
						<value>1 - Poor</value>
					</text>
					<text id="question2-two-label">
						<value>2 - Needs Improvement</value>
					</text>
					<text id="question2-three-label">
						<value>3 - Average</value>
					</text>
					<text id="question2-four-label">
						<value>4 - Good</value>
					</text>
					<text id="question2-five-label">
						<value>5 - Exceptional</value>
					</text>
					<text id="comment-label">
						<value>Enter some text:</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/basics/comment" nodeset="/data/case/update/comment" relevant="count(/data/basics/comment) &gt; 0"/><bind calculate="/data/last_inspection" nodeset="/data/case/update/last_inspection" relevant="count(/data/last_inspection) &gt; 0"/><bind calculate="/data/basics/rate_site" nodeset="/data/case/update/rate_site" relevant="count(/data/basics/rate_site) &gt; 0"/><setvalue event="xforms-ready" ref="/data/basics/comment" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/comment"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/><orx:pollsensor event="xforms-ready" ref="/data/meta/location"/><bind nodeset="/data/meta/location" type="geopoint"/></model>
	</h:head>
	<h:body>
		<group ref="/data/basics" appearance="field-list">
			<label ref="jr:itext('basics-label')"/>
			<select1 ref="/data/basics/rate_site">
				<label ref="jr:itext('question2-label')"/>
				<item>
					<label ref="jr:itext('question2-one-label')"/>
					<value>one</value>
				</item>
				<item>
					<label ref="jr:itext('question2-two-label')"/>
					<value>two</value>
				</item>
				<item>
					<label ref="jr:itext('question2-three-label')"/>
					<value>three</value>
				</item>
				<item>
					<label ref="jr:itext('question2-four-label')"/>
					<value>four</value>
				</item>
				<item>
					<label ref="jr:itext('question2-five-label')"/>
					<value>five</value>
				</item>
			</select1>
			<input ref="/data/basics/comment">
				<label ref="jr:itext('comment-label')"/>
			</input>
		</group>
	</h:body>
</h:html>