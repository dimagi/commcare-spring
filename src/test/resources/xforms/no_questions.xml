<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml"
    xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/jr/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <h:body>
        <group appearance="field-list" ref="/data/a_group">
            <label ref="jr:itext('a_group-label')" />
        </group>
    </h:body>
    <h:head>
        <h:title>Survey</h:title>
        <model>
            <bind calculate="&quot;hidden_in_root&quot;" nodeset="/data/hidden_in_root" />
            <bind nodeset="/data/a_group" />
            <bind calculate="&quot;hidden_in_group&quot;" nodeset="/data/a_group/hidden_in_group" />
            <bind nodeset="/data/meta/timeStart" type="xsd:dateTime" />
            <bind nodeset="/data/meta/timeEnd" type="xsd:dateTime" />
            <instance>
                <data name="Survey" uiVersion="1" version="5"
                    xmlns="http://openrosa.org/formdesigner/179475ED-0A85-4360-852B-4D8AD373076E">
                    <a_group>
                        <hidden_in_group />
                    </a_group>
                    <hidden_in_root />
                    <orx:meta xmlns:cc="http://commcarehq.org/xforms">
                        <cc:appVersion />
                        <orx:deviceID />
                        <orx:instanceID />
                        <orx:timeEnd />
                        <orx:timeStart />
                        <orx:userID />
                        <orx:username />
                    </orx:meta>
                </data>
            </instance>
            <instance id="commcaresession" src="jr://instance/session" />
            <itext>
                <translation default="" lang="en">
                    <text id="a_group-label">
                        <value>A GROUP</value>
                    </text>
                </translation>
            </itext>
            <setvalue event="xforms-ready" ref="/data/meta/deviceID"
                value="instance('commcaresession')/session/context/deviceid" />
            <setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()" />
            <setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()" />
            <setvalue event="xforms-ready" ref="/data/meta/username"
                value="instance('commcaresession')/session/context/username" />
            <setvalue event="xforms-ready" ref="/data/meta/userID"
                value="instance('commcaresession')/session/context/userid" />
            <setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()" />
            <setvalue event="xforms-ready" ref="/data/meta/appVersion"
                value="instance('commcaresession')/session/context/appversion" />
        </model>
    </h:head>
</h:html>
